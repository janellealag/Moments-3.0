   <!--==========================
  modal for login admin
============================-->  
<div class="modal fade" id="AdminLogin" tabindex="-1" role="dialog" aria-labelledby="modalLabel" aria-hidden="true">
  <div class="modal-dialog">
	<div class="modal-content">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">Ã—</span><span class="sr-only">Close</span></button>
			<h3 class="modal-title" id="lineModalLabel">ADMIN</h3>
		</div>
		<div class="modal-body">
			<form method="post" action="/login/adminLogin">
              <div class="form-group">
				<label for="Email">Email Address*</label>
                <input type="email" class="form-control" id="Email" name="email" placeholder="Email(eg. juandc@gmail.com)" required>
				<br>
				<label for="Password">Password*</label>
                <input type="password" class="form-control" name="password" id="Password" required pattern="[A-Za-z0-9]+" title="Please enter your correct password.">
				<div align="right"><input type="checkbox" onclick="AdminShowPassword()">Show Password</div>
				

<script>
function AdminShowPassword() {
    var x = document.getElementById("Password");
    if (x.type === "password") {
        x.type = "text";
    } else {
        x.type = "password";
    }
}
</script>
				<hr>
              <center><button type="submit" class="btn btn-services">Log In</button><center>
           </div>
                  </form>
		
	</div>
